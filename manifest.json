{
    "name": "QR CODE GENERATOR",
    "version": "1.0",
    "description": "Generate qr code from url",
    "permissions": ["activeTab", "declarativeContent", "storage"],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "page_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/qr-code-16.png",
            "32": "images/qr-code-32.png",
            "48": "images/qr-code-48.png",
            "128": "images/qr-code-128.png"
        }
    },
    "icons": {
        "16": "images/qr-code-16.png",
        "32": "images/qr-code-32.png",
        "48": "images/qr-code-48.png",
        "128": "images/qr-code-128.png"
    },
    "manifest_version": 2
}